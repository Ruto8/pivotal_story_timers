{
 "name": "Pivotal Story Timers",
 "content_scripts": [
   {
     "matches": ["https://www.pivotaltracker.com/n/projects/*"],
     "js": ["main.js"]
   }
 ],
  "version": "0.0.2",
  "description": "Display story cycle time in Pivotal",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
